type Author {
    id: ID!
    name: String!
    age: Int!
    tutorials: [Tutorial!]!
}

type Query {
    findAllAuthors: [Author!]!
}

type Mutation {
    createAuthor(authorInput: AuthorInput): Author
    updateAuthor(authorUpdateInput: AuthorUpdateInput): Author
}

input AuthorInput{
    name: String
    age: Int
}

input AuthorUpdateInput{
    id: ID
    name: String
    age: Int
}